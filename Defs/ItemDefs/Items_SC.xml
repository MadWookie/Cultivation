<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef>
    <defName>SC_Compost</defName>
    <thingClass>Thing</thingClass>
    <category>Item</category>
    <selectable>true</selectable>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>true</alwaysHaulable>
    <drawGUIOverlay>true</drawGUIOverlay>
    <label>compost</label>
    <description>Compost is used to make Fertilizer.</description>
    <graphicData>
      <texPath>Compost</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <rotatable>false</rotatable>
    <stackLimit>75</stackLimit>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MarketValue>1</MarketValue>
      <Mass>3.0</Mass>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>2.0</Flammability>
      <Beauty>-10</Beauty>
      <Cleanliness>-5</Cleanliness>
    </statBases>
    <techLevel>Medieval</techLevel>
    <researchPrerequisites>
      <li>SimpleCultivation</li>
    </researchPrerequisites>
  </ThingDef>

  <ThingDef>
    <defName>SC_Fertilizer</defName>
    <thingClass>Thing</thingClass>
    <category>Item</category>
    <selectable>true</selectable>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>true</alwaysHaulable>
    <drawGUIOverlay>true</drawGUIOverlay>
    <label>fertilizer</label>
    <description>Fertilizer is used to make cultivated soil for farming.</description>
    <graphicData>
      <texPath>Fertilizer</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <rotatable>false</rotatable>
    <stackLimit>10</stackLimit>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MarketValue>12</MarketValue>
      <Mass>5.0</Mass>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>4.0</Flammability>
    </statBases>
    <techLevel>Industrial</techLevel>
    <researchPrerequisites>
      <li>Cultivation</li>
    </researchPrerequisites>
  </ThingDef>

</Defs>